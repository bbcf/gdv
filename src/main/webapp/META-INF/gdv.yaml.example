################################################################
################################################################
##############   GDV CONFIGURATION FILE   ######################
################################################################
################################################################
### File in YAML format : http://www.yaml.org/
### email yohan.jarosz[at]epfl.ch for more information

################################################################
###################   SERVER CONFIG   ##########################
################################################################


# Server URL of the application.
# (where tomcat point to)

gdv_tomcat_server : http://localhost


# Proxy to access application.
# If not, put the same url as 'gdv_tomcat_server'.

gdv_proxy_url : http://localhost


# Installation path of gdv (absolute)

gdv_working_directory : $GDV_HOME


# Will match the path in the url to the application

gdv_version : gdv



################################################################
###########################   PSQL  ############################
################################################################


# Name of the database

psql_db : gdv


# Name of the user

psql_user : postgres


# Password

psql_pwd : postgres_passwd



################################################################
##################   OTHERS PARAMETERS    ######################
###   Don't change them unless you know what you are doing   ###
################################################################


# Url where to fecth static,public ressources.

gdv_public_url : /public


# GDV password to enable post access

gdv_post_access : gdv_post


# JBrowse browserRoot. The same	as gdv_public_url,

jb_browser_root : ../../tracks/

# JBrowse dataRoot (same)

jb_data_root : ../../tracks/
